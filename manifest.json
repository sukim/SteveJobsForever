{
  "name": "SteveJobsForever",
  "description": "Replacing Tim Cook to Steve Jobs",
  "version": "1.0",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
      "default_title": "Steve Jobs Forever",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,

    "web_accessible_resources": [
        "popup.js",
        "manifest.json",
        "steve/*"
    ]
}